<ripple
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:color="#66FFFFFF"
    tools:targetApi="lollipop">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <gradient
